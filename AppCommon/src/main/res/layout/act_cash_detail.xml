<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/sub_bg"
    android:fitsSystemWindows="@bool/fitsSystemWindows"
    android:orientation="vertical">

    <include layout="@layout/app_layout_common_title" />

    <LinearLayout
        android:id="@+id/rootView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="visible">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:scrollbars="none">

            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/sub_bg"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/line_liushui"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/margin_45dp"
                    android:background="@color/white"
                    android:clickable="true"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="@dimen/margin_10dp"
                    android:paddingRight="@dimen/margin_10dp">

                    <TextView
                        android:layout_width="@dimen/margin_60dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="0dip"
                        android:clickable="false"
                        android:gravity="right|center_vertical"
                        android:text="流水号"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_15sp"
                        android:visibility="visible" />

                    <TextView
                        android:id="@+id/ed_liushui"
                        android:layout_width="0dip"
                        android:layout_height="fill_parent"
                        android:layout_marginBottom="5dip"
                        android:layout_marginLeft="0dip"
                        android:layout_marginRight="0dip"
                        android:layout_marginTop="5dip"
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:clickable="false"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="10dip"
                        android:singleLine="true"
                        android:textColor="@color/grey"
                        android:textColorHint="@color/grey_40"
                        android:textSize="@dimen/text_size_15sp"
                        android:visibility="visible" />

                </LinearLayout>

                <include layout="@layout/app_layout_common_line" />

                <LinearLayout
                    android:id="@+id/line_bank"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/margin_45dp"
                    android:background="@color/white"
                    android:clickable="true"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="@dimen/margin_10dp"
                    android:paddingRight="@dimen/margin_10dp">

                    <TextView
                        android:layout_width="@dimen/margin_60dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="0dip"
                        android:clickable="false"
                        android:gravity="right|center_vertical"
                        android:text="银行"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_15sp"
                        android:visibility="visible" />

                    <TextView
                        android:id="@+id/ed_bank"
                        android:layout_width="0dip"
                        android:layout_height="fill_parent"
                        android:layout_marginBottom="5dip"
                        android:layout_marginLeft="0dip"
                        android:layout_marginRight="0dip"
                        android:layout_marginTop="5dip"
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:clickable="false"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="10dip"
                        android:singleLine="true"
                        android:textColor="@color/grey"
                        android:textColorHint="@color/grey_40"
                        android:textSize="@dimen/text_size_15sp"
                        android:visibility="visible" />


                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/line_divider_bank">
                    <include layout="@layout/app_layout_common_line"
                       />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/line_card"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/margin_45dp"
                    android:background="@color/white"
                    android:clickable="true"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="@dimen/margin_10dp"
                    android:paddingRight="@dimen/margin_10dp">

                    <TextView
                        android:layout_width="@dimen/margin_60dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="0dip"
                        android:clickable="false"
                        android:gravity="right|center_vertical"
                        android:text="卡号"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_15sp"
                        android:visibility="visible" />

                    <TextView
                        android:id="@+id/ed_card"
                        android:layout_width="0dip"
                        android:layout_height="fill_parent"
                        android:layout_marginBottom="5dip"
                        android:layout_marginLeft="0dip"
                        android:layout_marginRight="0dip"
                        android:layout_marginTop="5dip"
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:clickable="false"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="10dip"
                        android:singleLine="true"
                        android:textColor="@color/grey"
                        android:textColorHint="@color/grey_40"
                        android:textSize="@dimen/text_size_15sp"
                        android:visibility="visible" />


                </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/line_divider_card">
                <include layout="@layout/app_layout_common_line"
                    />
            </LinearLayout>
                <LinearLayout
                    android:id="@+id/line_balance"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/margin_45dp"
                    android:background="@color/white"
                    android:clickable="true"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="@dimen/margin_10dp"
                    android:paddingRight="@dimen/margin_10dp">

                    <TextView
                        android:layout_width="@dimen/margin_60dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="0dip"
                        android:clickable="false"
                        android:gravity="right|center_vertical"
                        android:text="出金额"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_15sp"
                        android:visibility="visible"
                        android:id="@+id/ed_balance_lable"
                        />

                    <TextView
                        android:id="@+id/ed_balance"
                        android:layout_width="0dip"
                        android:layout_height="fill_parent"
                        android:layout_marginBottom="5dip"
                        android:layout_marginLeft="0dip"
                        android:layout_marginRight="0dip"
                        android:layout_marginTop="5dip"
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:clickable="false"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="10dip"
                        android:singleLine="true"
                        android:textColor="@color/grey"
                        android:textColorHint="@color/grey_40"
                        android:textSize="@dimen/text_size_15sp"
                        android:visibility="visible" />


                </LinearLayout>

                <include layout="@layout/app_layout_common_line" />
                <LinearLayout
                    android:id="@+id/line_time"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/margin_45dp"
                    android:background="@color/white"
                    android:clickable="true"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="@dimen/margin_10dp"
                    android:paddingRight="@dimen/margin_10dp">

                    <TextView
                        android:layout_width="@dimen/margin_60dp"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="0dip"
                        android:clickable="false"
                        android:gravity="right|center_vertical"
                        android:text="处理时间"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_15sp"
                        android:visibility="visible" />

                    <TextView
                        android:id="@+id/ed_time"
                        android:layout_width="0dip"
                        android:layout_height="fill_parent"
                        android:layout_marginBottom="5dip"
                        android:layout_marginLeft="0dip"
                        android:layout_marginRight="0dip"
                        android:layout_marginTop="5dip"
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:clickable="false"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="10dip"
                        android:singleLine="true"
                        android:textColor="@color/grey"
                        android:textColorHint="@color/grey_40"
                        android:textSize="@dimen/text_size_15sp"
                        android:visibility="visible" />

                </LinearLayout>
                <include layout="@layout/app_layout_common_line" />
                <LinearLayout
                    android:id="@+id/line_state"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:clickable="true"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="@dimen/margin_10dp"
                    android:paddingRight="@dimen/margin_10dp"
                    android:minHeight="@dimen/margin_45dp">

                    <TextView
                        android:layout_width="@dimen/margin_60dp"
                        android:layout_height="@dimen/margin_45dp"
                        android:layout_marginLeft="0dip"
                        android:clickable="false"
                        android:gravity="right|center_vertical"
                        android:text="处理状态"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_15sp"
                        android:visibility="visible" />

                    <TextView
                        android:id="@+id/ed_state"
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dip"
                        android:layout_marginLeft="0dip"
                        android:layout_marginRight="0dip"
                        android:layout_marginTop="5dip"
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:clickable="false"
                        android:gravity="left|center_vertical"
                        android:paddingLeft="10dip"
                        android:paddingRight="10dip"
                        android:textColor="@color/grey"
                        android:textColorHint="@color/grey_40"
                        android:textSize="@dimen/text_size_15sp"
                        android:visibility="visible"/>

                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</LinearLayout>