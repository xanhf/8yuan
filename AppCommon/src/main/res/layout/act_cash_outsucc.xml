<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/sub_bg"
    android:fitsSystemWindows="@bool/fitsSystemWindows"
    android:orientation="vertical">
    <include layout="@layout/app_layout_common_title" />
    <ImageView
        android:layout_width="@dimen/margin_90dp"
        android:layout_height="@dimen/margin_90dp"
        android:src="@drawable/ic_card_status_ok"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/margin_60dp"
        android:layout_marginBottom="@dimen/margin_20dp"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_size_15sp"
        android:textColor="@color/app_common_content"
        android:text="@string/cashout_success_asctract"
        android:gravity="center"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="@dimen/text_size_12sp"
        android:textColor="@color/grey"
        android:text="@string/cashout_success_content"
        android:gravity="center|left"
        android:layout_marginTop="@dimen/margin_20dp"
        android:layout_marginLeft="@dimen/margin_60dp"
        android:layout_marginRight="@dimen/margin_60dp"
        android:id="@+id/text_tips"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1.0"/>
    <include
        layout="@layout/app_copyright_layout"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/margin_20dp"/>
</LinearLayout>