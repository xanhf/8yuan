<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_selected="true">
        <shape>
            <stroke android:width="0.5dip" android:color="@color/tt_line_selected" />
            <solid android:color="@color/tt_solid_selected" />
            <!--<corners android:bottomRightRadius="3dip" android:topRightRadius="3dip" />-->
        </shape>

    </item>
    <item android:state_selected="true">
        <shape>
            <stroke android:width="0.5dip" android:color="@color/tt_line_selected" />
            <solid android:color="@color/tt_solid_selected" />
            <!--<corners android:bottomRightRadius="3dip" android:topRightRadius="3dip" />-->
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <stroke android:width="0.5dip" android:color="@color/tt_line_normal" />
            <solid android:color="@color/tt_solid_normal" />
            <!--<corners android:bottomRightRadius="3dip" android:topRightRadius="3dip" />-->
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="0.5dip" android:color="@color/tt_line_normal" />
            <solid android:color="@color/tt_solid_normal" />
            <!--<corners android:bottomRightRadius="3dip" android:topRightRadius="3dip" />-->
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="0.5dip" android:color="@color/tt_line_normal" />
            <solid android:color="@color/tt_solid_normal" />
            <!--<corners android:bottomRightRadius="3dip" android:topRightRadius="3dip" />-->
        </shape>
    </item>
</selector>